{
  "name": "tiation-ai-agents",
  "version": "1.0.0",
  "description": "Enterprise-Grade AI Automation Platform",
  "main": "src/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t tiation-ai-agents .",
    "docker:run": "docker run -p 3000:3000 -p 8080:8080 tiation-ai-agents",
    "start:monitoring": "node dist/services/MonitoringService.js",
    "start:integration": "node dist/integrations/LiberationSystemIntegration.js"
  },
  "keywords": [
    "ai",
    "agents",
    "automation",
    "enterprise",
    "liberation-system",
    "monitoring",
    "dashboard"
  ],
  "author": "Tiation",
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.5",
    "axios": "^1.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.0",
    "recharts": "^2.7.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.24",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiation/tiation-ai-agents.git"
