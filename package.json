{
  "name": "tiation-ai-agents",
  "version": "1.0.0",
  "description": "Intelligent automation that empowers, not replaces - Enterprise AI solutions",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "dev": "vite",
    "build": "vite build",
    "build:api": "npm run copy-files",
    "build:web": "cd web && npm run build",
    "copy-files": "cp -r src/* dist/",
    "test": "vitest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docker:build": "docker build -t tiation-ai-agents .",
    "docker:run": "docker run -p 3000:3000 tiation-ai-agents",
    "deploy": "npm run build && gh-pages -d dist",
    "setup": "npm install && cd web && npm install && cd ../TiationAIAgentsMobile && npm install",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:analyze": "vite build --mode production --report",
    "test:coverage": "vitest --coverage",
    "preview": "vite preview",
    "deploy:netlify": "npm run build && npx netlify-cli deploy --prod --dir dist",
    "deploy:surge": "npm run build:prod && npx surge dist --domain tiation-ai-agents.surge.sh",
    "deploy:surge:custom": "npm run build:prod && npx surge dist",
    "deploy:all": "npm run deploy:surge && npm run deploy",
    "predeploy": "npm run lint && npm run test && npm run build:prod",
    "postdeploy": "npm run health-check",
    "surge:login": "npx surge login",
    "surge:whoami": "npx surge whoami",
    "surge:list": "npx surge list",
    "surge:teardown": "npx surge teardown tiation-ai-agents.surge.sh",
    "health-check": "curl -f https://tiation-ai-agents.surge.sh/health || exit 1",
    "lighthouse": "npx lighthouse https://tiation-ai-agents.surge.sh --output html --output-path ./lighthouse-report.html",
    "security-scan": "npx audit-ci --config .audit-ci.json",
    "performance-test": "npx pagespeed-insights https://tiation-ai-agents.surge.sh"
  },
  "keywords": [
    "ai",
    "automation",
    "enterprise",
    "agents",
    "saas",
    "mobile-app",
    "tiation"
  ],
  "author": {
    "name": "Tia Astor",
    "email": "tiatheone@protonmail.com",
    "url": "https://tiation.github.io/TiaAstor/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiation/tiation-ai-agents.git"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.5",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "framer-motion": "^10.16.0",
    "helmet": "^7.2.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.263.1",
    "mongoose": "^7.8.7",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.4",
    "redis": "^4.6.7",
    "socket.io": "^4.7.2",
    "stripe": "^12.18.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/uuid": "^9.0.2",
    "eslint": "^8.45.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "surge": "^0.24.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
